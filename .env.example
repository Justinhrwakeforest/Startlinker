# Environment variables for StartupHub
# Copy this file to .env and update the values

# Django settings
DJANGO_SECRET_KEY=your-secret-key-here
DJANGO_DEBUG=True

# Database settings
DB_NAME=startup_hub
DB_USER=startup_user
DB_PASSWORD=your-password-here
DB_HOST=localhost
DB_PORT=5432

# URLs
FRONTEND_URL=http://localhost:3000
BACKEND_URL=http://localhost:8000

# Additional CORS origins (comma-separated)
CORS_ADDITIONAL_ORIGINS=
CSRF_ADDITIONAL_ORIGINS=

# Redis settings (for channels)
REDIS_HOST=127.0.0.1
REDIS_PORT=6379

# Email settings
EMAIL_BACKEND=django.core.mail.backends.console.EmailBackend
EMAIL_HOST=
EMAIL_PORT=587
EMAIL_USE_TLS=True
EMAIL_HOST_USER=
EMAIL_HOST_PASSWORD=

# GitHub OAuth settings
GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret
GITHUB_REDIRECT_URI=http://localhost:8000/api/v1/integrations/github/callback/
GITHUB_TOKEN_ENCRYPTION_KEY=generate-with-fernet-generate-key


# OpenAI settings (for AI features)
OPENAI_API_KEY=your-openai-api-key

# Anthropic settings (for AI features)
ANTHROPIC_API_KEY=your-anthropic-api-key